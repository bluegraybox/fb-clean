{
	"manifest_version": 2,
	"name": "FB Clean",
	"version": "0.1.0",
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["https://*.facebook.com/*"],
			"run_at": "document_end",
			"css": ["facebook.css"],
			"js": ["facebook.js"]
		},
		{
			"matches": ["https://*.mewe.com/*"],
			"run_at": "document_end",
			"css": ["mewe.css"],
			"js": ["mewe.js"]
		}
	]
}
